        "jsonBody": "={\n  \"phoneNumber\": \"{{ $node['Split Customers'].json.phone }}\",\n  \"updates\": {\n    \"D\": \"{{ $json.interestedInOriginal ? 'Yes' : 'No' }}\",\n    \"E\": \"{{ $json.appointmentDateTime || '' }}\",\n    \"F\": \"{{ $json.interestedInSimilar ? 'Yes' : 'No' }}\",\n    \"G\": \"{{ $json.email || '' }}\",\n    \"lastCallDate\": \"{{ new Date().toISOString().split('T')[0] }}\",\n    \"callStatus\": \"{{ $json.status }}\",\n    \"callDuration\": \"{{ $json.duration }}\",\n    \"sentiment\": \"{{ $json.sentiment }}\"\n  }\n}",
        "options": {}
