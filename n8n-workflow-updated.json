        "authentication": "genericCredentialType",
        "genericAuthType": "httpHeaderAuth",
        "sendBody": true,
        "specifyBody": "json",
        "jsonBody": "={\n  \"phoneNumber\": \"{{ $json.phone }}\",\n  \"customerData\": {\n    \"id\": \"{{ $json.id || $json.customerId }}\",\n    \"name\": \"{{ $json.name }}\",\n    \"phone\": \"{{ $json.phone }}\",\n    \"carModel\": \"{{ $json.carModel }}\",\n    \"dealershipName\": \"{{ $node['Set Configuration'].json.DEALERSHIP_NAME }}\",\n    \"enquiryDate\": \"{{ $json.enquiryDate || new Date().toISOString().split('T')[0] }}\",\n    \"email\": \"{{ $json.email || '' }}\"\n  }\n}",
        "options": {}
      },
